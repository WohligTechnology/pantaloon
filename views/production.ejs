<!DOCTYPE html>
<html lang="en" ng-app="firstapp">

<head ng-controller="headerctrl">
    <meta charset="utf-8">
    <base href="/">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="shortcut icon" href="frontend/img/favicon.ico" />
    <title ng-bind="'Pantaloons | '+template.title">
        <%= ((typeof title) == "undefined") ? "Pantaloons" : title  %>
    </title>

    <!-- SEO -->
    <meta name="author" content="Pantaloons">
    <meta name="description" content="<%= ((typeof description) == "undefined") ? "Pantaloons " : description  %>">
    <meta name="keywords" content="<%= ((typeof keywords) == "undefined") ? "Pantaloons " : keywords  %>">

    <!-- Facebook -->
    <meta property="og:site_name" content="Pantaloons">
    <meta property="og:title" content="<%= ((typeof title) == "undefined") ? "Pantaloons " : title  %>" />
    <meta property="og:description" content="<%= ((typeof description) == "undefined") ? "Pantaloons " : description  %>" />
    <meta property="og:image" content="<%= ((typeof image) == "undefined") ? "Pantaloons " : image  %>">
    <meta property="og:type" content="website" />
    <meta property="og:url" content="<%= ((typeof url) == "undefined") ? "Pantaloons " : url  %>" />


    <!-- Twitter -->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@wohligtechnology">
    <meta name="twitter:title" content="<%= ((typeof title) == "undefined") ? "Pantaloons " : title  %>">
    <meta name="twitter:description" content="<%= ((typeof description) == "undefined") ? "Official Pantaloons Website " : description  %>">
    <meta name="twitter:image" content="<%= ((typeof image) == "undefined") ? "Pantaloons " : image  %>">

    <link rel="canonical" href="<%= ((typeof url) == "undefined") ? "Pantaloons " : url  %>">

    <!--[if IE]> <script src="https://cdn.jsdelivr.net/html5shiv/3.7.2/html5shiv.min.js"></script> <script src="https://cdn.jsdelivr.net/respond/1.4.2/respond.min.js"></script> <![endif]-->
    <!--[if IE]> <script src="https://cdn.jsdelivr.net/html5shiv/3.7.2/html5shiv.min.js"></script> <script src="https://cdn.jsdelivr.net/respond/1.4.2/respond.min.js"></script> <![endif]-->
</head>

<body>
    <div class="repeated-item" ui-view></div>

    <link rel="stylesheet" type="text/css" href="frontend/css/main.min.css" />
    <script>
        var adminurl = "<%= ((typeof adminurl) == "undefined") ? "" : adminurl  %>";
        var isproduction = true;
    </script>

    <script src="frontend/js/main.min.js"></script>
    <script>
        (function(i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] || function() {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
        ga('create', 'UA-77872125-3', {
            'cookieDomain': 'none'
        });
    </script>
</body>

</html>
